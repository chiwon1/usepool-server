{
  "name": "usepool-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./dist/bin/www.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check --watch",
    "dev": "NODE_ENV=development npm run build && nodemon nodemon ./dist/bin/www.js",
    "build": "babel src --out-dir dist --extensions \".js,.ts,.tsx\" --source-maps inline"
  },
  "dependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.10.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "helmet": "^4.6.0",
    "http-errors": "^1.6.3",
    "morgan": "~1.9.1",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.13"
  }
}
