{
  "name": "usepool-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./dist/bin/www.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check --watch",
    "dev": "NODE_ENV=development npm run build && nodemon ./dist/bin/www.js",
    "build": "babel src --out-dir dist --extensions \".js,.ts,.tsx\" --source-maps inline",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.22.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "helmet": "^4.6.0",
    "http-errors": "^1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.8",
    "morgan": "~1.9.1",
    "socket.io": "^4.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0"
  },
  "devDependencies": {
    "typescript": "^4.4.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.13",
    "prettier": "2.4.1"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write ./src",
      "eslint --fix ./src"
    ]
  }
}
